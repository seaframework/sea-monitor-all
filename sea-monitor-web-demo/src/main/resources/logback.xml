<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <logger name="com.github.spy" level="DEBUG"/>

    <logger name="com.alibaba.dubbo" level="DEBUG"/>
    <logger name="org.springframework" level="INFO"/>
    <logger name="org.springboot.sample" level="TRACE"/>
    <logger name="org.mybatis" level="INFO"/>
    <logger name="java.sql" level="INFO"/>
    <logger name="druid.sql" level="DEBUG"/>
    <logger name="org.apache.juli" level="WARN"/>
    <logger name="org.apache.http" level="INFO"/>
    <logger name="org.apache.ibatis" level="INFO"/>


    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <!--yyyy-MM-dd -->
            <pattern>%d{HH:mm:ss.SSS} %level [%thread] [%class{36}.%M:%line] - %m %n</pattern>
        </encoder>
    </appender>
    <appender name="SEA_MONITOR" class="com.github.seaframework.monitor.plugin.logback.SeaMonitorAppender"></appender>


    <root level="DEBUG">
        <appender-ref ref="STDOUT"/>
        <appender-ref ref="SEA_MONITOR"/>
    </root>
</configuration>
